#!/usr/bin/python3.5

def main():
    the_string = "abcde"
    result = isUniqueChars(the_string)
    print("\n{}".format(result))

def isUniqueChars(the_string):
    for char in range(len(the_string)):
        
        return False if char in the_string else print("Keep going")
    return True
if __name__ == "__main__": main()