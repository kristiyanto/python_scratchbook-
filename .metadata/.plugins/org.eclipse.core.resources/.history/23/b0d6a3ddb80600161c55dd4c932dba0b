#!/usr/bin/python3.5

def main():
    the_string = "abcde"
    result = isUniqueChars(the_string)
    print("\n{}".format(result))

def isUniqueChars(the_string):
    charByte = bytearray()
    for char in the_string:
        print(char, end="_")
        charByte.append(ord(char))
    return(charByte)

if __name__ == "__main__": main()