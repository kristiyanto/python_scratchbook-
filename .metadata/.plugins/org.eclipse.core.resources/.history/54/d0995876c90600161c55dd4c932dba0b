#! /bin/usr/python3.5

def main():
    s = "aaaaabbbcc"
    result = compressThis(s)
    print("{}".format(result))
    
def compressThis(s):
    r = list(s[1])
    for i in range(len(s)):
        cons = 0
        if s[i] == s[i]: cons +=1
        elif cons>1 r.append(cons)
        else: r.append(s[i])
            
    return r
if __name__ =="__main__": main()